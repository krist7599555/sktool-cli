#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/krist7599555/Github/sktool/node_modules/.pnpm/fx@28.0.0/node_modules/fx/node_modules:/Users/krist7599555/Github/sktool/node_modules/.pnpm/fx@28.0.0/node_modules:/Users/krist7599555/Github/sktool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/krist7599555/Github/sktool/node_modules/.pnpm/fx@28.0.0/node_modules/fx/node_modules:/Users/krist7599555/Github/sktool/node_modules/.pnpm/fx@28.0.0/node_modules:/Users/krist7599555/Github/sktool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../fx@28.0.0/node_modules/fx/index.js" "$@"
else
  exec node  "$basedir/../../../../../fx@28.0.0/node_modules/fx/index.js" "$@"
fi
